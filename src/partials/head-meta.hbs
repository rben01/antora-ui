{{!-- Add additional meta tags here --}}
<link rel="stylesheet" href="https://iosevka-rltb.pages.dev/dist/iosevka-rltb-mono/iosevka-rltb-mono-cloudflare.css">
<meta name="theme-color" content="#0970c4" media="(prefers-color-scheme: light)">
<meta name="theme-color" content="#0967b4" media="(prefers-color-scheme: dark)">
{{!-- preload home icons. technically the home-o icons aren't necessary on the home page
itself, but whatever--}}
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/light-theme/home.svg" media="(prefers-color-scheme: light)">
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/light-theme/home-o.svg" media="(prefers-color-scheme: light)">
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/dark-theme/home.svg" media="(prefers-color-scheme: dark)">
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/dark-theme/home-o.svg" media="(prefers-color-scheme: dark)">
{{!-- preload nav bar icons on mobile --}}
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/light-theme/back.svg"
	media="(prefers-color-scheme: light) and (max-width: 1023.5px)">
<link rel="preload" as="image" href="{{{uiRootPath}}}/img/dark-theme/back.svg"
	media="(prefers-color-scheme: dark) and (max-width: 1023.5px)">
